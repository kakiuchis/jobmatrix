<p id="notice"><%= notice %></p>

<h1>Listing Levels</h1>

<table class="table">
  <thead>
    <tr>
      <th>Field</th>
      <th>Height</th>
      <th>Mintariff</th>
      <th>Maxtariff</th>
      <th>Name</th>
      <th>Skill</th>
      <th>Tool</th>
      <th>Certificate</th>
      <th>Field</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @levels.each do |level| %>
      <tr>
        <td><%= level.field %></td>
        <td><%= level.height %></td>
        <td><%= level.mintariff %></td>
        <td><%= level.maxtariff %></td>
        <td><%= level.name %></td>
        <td><%= level.skill %></td>
        <td><%= level.tool %></td>
        <td><%= level.certificate %></td>
        <td><%= level.field_id %></td>
        <td><%= link_to 'Show', field_level_path(@field, level) %></td>
        <td><%= link_to 'Edit', edit_field_level_path(@field, level) %></td>
        <td><%= link_to 'Destroy', field_level_path(@field, level), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Level', new_field_level_path %>
