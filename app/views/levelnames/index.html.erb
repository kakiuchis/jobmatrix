<p id="notice"><%= notice %></p>

<h1>Listing Levelnames</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @levelnames.each do |levelname| %>
      <tr>
        <td><%= levelname.code %></td>
        <td><%= levelname.name %></td>
        <td><%= link_to 'Edit', edit_levelname_path(levelname) %></td>
        <td><%= link_to 'Destroy', levelname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Levelname', new_levelname_path %>
