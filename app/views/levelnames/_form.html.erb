<%= form_for(@levelname) do |f| %>
  <%= f.error_messages! %>
  <div class="field">
    <%= f.label :code %><br>
    <%= f.number_field :code, class: 'form-control' %>
  </div>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="actions">
    <%= f.submit '決定', class: 'btn btn-primary btn-block' %>
  </div>
<% end %>
